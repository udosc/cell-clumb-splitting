	    	description="Image Processing and Mining Nodes from the University of Konstanz"
			level-id="master"
	        name="Master Thesis"
	        path="/">
	    </category>
    
    </extension>
   
   <extension point="org.knime.workbench.repository.nodesets">
        <nodeset 
        	expert-flag="false" 
        	factory-class="org.knime.knip.clump.nodes.ClumpSplittingNodeSetFactory" 
        	id="org.knime.knip.clumb.nodes.ClumbSplittingNodeSetFactory">
