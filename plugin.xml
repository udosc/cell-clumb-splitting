    <extension point="org.knime.workbench.repository.nodes">  
      	
		<node 
			category-path="/master" 
			expert-flag="false" 
			factory-class="org.knime.knip.clump.nodes.curvature.CurvatureNodeFactory" 
			id="org.knime.knip.clump.nodes.curvature.CurvatureNodeFactory">
		</node>
		
		<node 
			category-path="/master" 
			expert-flag="false" 
			factory-class="org.knime.knip.clump.nodes.ftcurvature.FTCurvatureNodeFactory" 
			id="org.knime.knip.clump.nodes.ftcurvature.FTCurvatureNodeFactory">
		</node>

	
		
	</extension>
   

