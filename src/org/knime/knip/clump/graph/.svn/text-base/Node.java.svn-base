package org.knime.knip.clump.graph;


/**
 * 
 * @author Udo Schlegel
 *
 */
public class Node{
	
	private final int m_index;
	
	private final long[] m_position;
	
	
	public Node(int index, long[] position){
		m_index = index;
		m_position = position;
	}
		
	public int getIndex(){
		return m_index;
	}
	
	public long[] getPosition(){
		return m_position;
	}

	@Override
	public String toString(){
		return m_index + " (" + m_position + ") ";
	}
}
